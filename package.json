{
  "name": "ai-bpr-app",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate:pg": "prisma generate --schema prisma/postgres/schema.prisma",
    "prisma:push:pg": "prisma db push --schema prisma/postgres/schema.prisma",
    "prisma:migrate:pg": "prisma migrate deploy --schema prisma/postgres/schema.prisma",
    "db:reset": "rimraf dev.db && prisma migrate reset --force && prisma db seed",
    "db:seed": "prisma db seed",
    "test": "jest",
    "e2e": "playwright test",
    "e2e:install": "playwright install --with-deps"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "@tanstack/react-query": "5.56.2",
    "bcryptjs": "2.4.3",
    "classnames": "2.5.1",
    "next": "14.2.5",
    "next-auth": "4.24.7",
    "openai": "4.56.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/react": "19.1.13",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "tailwindcss": "3.4.10",
    "ts-jest": "29.2.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
